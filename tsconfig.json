{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Menu.jsx",
    "pages/index.jsx"
, "src/components/DarkLight.jsx", "src/components/Menu.jsx", "src/pages/blog/[post].jsx", "src/pages/blog/tags/[[...slug]].jsx"  ],
  "exclude": [
    "node_modules"
  ]
}